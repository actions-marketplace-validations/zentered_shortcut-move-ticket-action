{
  "name": "shortcut-move-ticket-action",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Move a ticket on Shortcut to another workflow/column",
  "keywords": [
    "shortcut",
    "workflow",
    "productivity"
  ],
  "homepage": "https://github.com/zentered/shortcut-move-ticket-action#readme",
  "bugs": {
    "url": "https://github.com/zentered/shortcut-move-ticket-action/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zentered/shortcut-move-ticket-action.git"
  },
  "license": "MIT",
  "author": "Zentered <hi@zentered.co> (https://zentered.co)",
  "contributors": [
    "Patrick Heneise (https://github.com/patrickheneise)"
  ],
  "main": "dist/index.js",
  "scripts": {
    "start": "node -r dotenv/config src/index.mjs",
    "build": "npm run lint && npm run test && npm run prepare",
    "postinstall": "husky install",
    "lint": "eslint .",
    "prepare": "ncc build src/index.js -o dist --source-map --license licenses.txt",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,json,md,yml}": [
      "prettier --write"
    ],
    "*.{mjs,js}": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.0",
    "got": "^12.0.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@vercel/ncc": "=0.33.3",
    "commitlint": "^16.2.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-config": "^27.5.1",
    "lint-staged": "^12.3.4",
    "pinst": "^2.1.6",
    "prettier": "^2.5.1"
  },
  "engines": {
    "node": "^16"
  },
  "release": {
    "branches": [
      "main"
    ]
  }
}
